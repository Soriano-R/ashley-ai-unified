{
  "name": "ashley-ai-unified",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:python\"",
    "dev:next": "next dev --turbopack",
    "dev:python": "cd python-service && source ../../chatbot_env/bin/activate && python main.py",
    "build": "next build",
    "start": "concurrently \"npm run start:next\" \"npm run start:python\"",
    "start:next": "next start",
    "start:python": "cd python-service && source ../../chatbot_env/bin/activate && python main.py",
    "lint": "eslint",
    "python:install": "source ../chatbot_env/bin/activate && pip install -r requirements.txt",
    "python:activate": "source ../chatbot_env/bin/activate"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
